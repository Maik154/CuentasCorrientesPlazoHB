<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.github.maik154.ejercicio5.Pojos">
    <class name="Movimiento" table="MOVIMIENTO">
        <composite-id>
            <key-many-to-one name="cuentaCorriente" column="NUMERO_CUENTA" class="CuentaCorriente"/>
            <key-many-to-one name="fechaOperacion" column="FECHA_OPERACION"/>
            <key-many-to-one name="hora" column="HORA"/>
        </composite-id>
        <property name="cliente" column="ID_CLIENTE"/>
        <property name="cantidad" column="CANTIDAD"/>
        <property name="operacion" column="OPERACION"/>
    </class>

</hibernate-mapping>